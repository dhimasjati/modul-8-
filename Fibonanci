import java.util.Scanner;


 public class Fibonanci {

    public static void main(String[] args) {
        System.out.print("Masukan angka : ");
		 Scanner sc = new Scanner (System.in);
		 int x = sc.nextInt();
		 int a=0;
		 int b=1;
		 int hasil=0;
		 for (int i=0;i<=x;i++) {
                     
			        
				a=a+b;
                                b=a-b;
                                hasil=hasil+a;
			}
		 System.out.print(hasil);
    }
    
    
}
